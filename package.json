{
  "name": "@elite-libs/rules-machine",
  "version": "1.0.3",
  "description": "A JSON-based Rules Engine. Serialize business logic into JSON to manage complexity & model larger workflows.",
  "type": "commonjs",
  "homepage": "https://github.com/elite-libs/rules-machine",
  "repository": {
    "type": "git",
    "url": "git://github.com/elite-libs/rules-machine.git"
  },
  "source": "./src/index.ts",
  "// exports": {
    ".": {
      "import": "./dist/index.cjs",
      "require": "./dist/main.cjs"
    },
    "./main": {
      "import": "./dist/index.cjs",
      "require": "./dist/main.cjs",
      "types": "./dist/main.d.ts"
    },
    "./legacy": {
      "import": "./dist/main.cjs",
      "require": "./dist/main.cjs"
    },
    "./src/index": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "main": "./dist/main.cjs",
  "module": "./dist/main.mjs",
  "types": "./dist/index.d.ts",
  "browser": "./dist/index.cjs",
  "private": false,
  "packageManager": "yarn@1.10.0",
  "engineStrict": true,
  "engines": {
    "yarn": ">=1.10.0",
    "node": ">=12.0.0"
  },
  "modern": "dist/modern.js",
  "legacy": "dist/legacy.js",
  "targets": {
    "types": {
      "source": "src/types.ts"
    },
    "modern": {
      "context": "browser",
      "optimize": false,
      "isLibrary": true,
      "outputFormat": "esmodule",
      "engines": {
        "node": ">= 12"
      }
    },
    "legacy": {
      "context": "browser",
      "optimize": false,
      "isLibrary": true,
      "outputFormat": "commonjs",
      "engines": {
        "browsers": "> 1.5%, last 2 versions, not dead"
      }
    }
  },
  "scripts": {
    "prepublishOnly": "yarn run build",
    "release:npm": "npm publish --access public --registry https://registry.npmjs.org/",
    "release:github": "npm publish --access public --registry https://npm.pkg.github.com/",
    "test": "yarn run build && jest",
    "test:coverage": "yarn run build && jest --coverage",
    "build": "npx rimraf ./dist/* && yarn run build:types && yarn run build:bundle",
    "build:bundle": "parcel build",
    "build:types": "tsc --downlevelIteration --isolatedModules false --declaration --declarationMap --noEmit false --module CommonJS --outDir dist/ts/"
  },
  "keywords": [
    "Rules Engine",
    "JSON Rules Engine",
    "JSON Rules",
    "JSON",
    "JSON-based",
    "JSON-based Rules Engine",
    "JSON-based Rules",
    "JSON-based Rules Engine",
    "YAML Rules Engine"
  ],
  "devDependencies": {
    "@parcel/packager-ts": "^2.3.1",
    "@parcel/transformer-typescript-types": "^2.3.1",
    "@types/debug": "^4.1.7",
    "@types/jest": "26.x.x",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.0",
    "jest": "26.x.x",
    "parcel": "^2.3.1",
    "ts-jest": "26.x.x",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "author": {
    "name": "@justsml",
    "url": "https://danlevy.net"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "debug": "^4.3.3",
    "expressionparser": "^1.1.5",
    "lodash": "^4.17.21",
    "ms": "^2.1.3"
  }
}
